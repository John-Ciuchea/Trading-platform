{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true
  },
  "projects": {
    "http": {
      "type": "application",
      "root": "src",
      "entryFile": "http",
      "sourceRoot": "src"
    },
    "subscriber": {
      "type": "application",
      "root": "src",
      "entryFile": "subscriber",
      "sourceRoot": "src"
    }
  }
}
